<?xml version="1.0"?>
<object name="portal_controlpanel" 
        meta_type="Plone Control Panel Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n" 
        i18n:domain="plone.app.workflowmanager">

    <configlet title="Non tracking Social Buttons"
               action_id="notracksocial"
               appId="wildcard.notrackingsocial" category="Products" condition_expr=""
    url_expr="string:${portal_url}/@@notracksocial-controlpanel"
    icon_expr="string:++resource++notracksocial/twitter.png"
    visible="True" i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
  
</object>